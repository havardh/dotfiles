[alias]
	o = checkout
	c = commit
	st = status
	cm = commit -m
	unstage = reset HEAD --
	last = log -1 HEAD --
	f = fetch
	p = pull

	exec = "!exec "
	recent = for-each-ref --sort=committerdate refs/heads/ --format='%(refname:short)'